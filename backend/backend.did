type FileInfo = 
 record {
   chunkCount: nat;
   contentType: text;
   name: text;
   size: nat;
 };
service : {
  getFileChunk: (text, nat) -> (opt blob) query;
  getFileInfo: (text) -> (opt FileInfo) query;
  listFiles: () -> (vec text) query;
  uploadFileChunk: (text, text, nat, nat, nat, vec nat8) -> ();
}
