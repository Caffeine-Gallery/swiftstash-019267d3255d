type FileInfo = 
 record {
   content: blob;
   contentType: text;
   name: text;
 };
service : {
  deleteFile: (text) -> ();
  getFileContent: (text) -> (opt blob) query;
  getFileInfo: (text) -> (opt FileInfo) query;
  listFiles: () -> (vec text) query;
  uploadFile: (text, text, vec nat8) -> (text);
}
