type FileInfo = 
 record {
   chunkCount: nat64;
   contentType: text;
   name: text;
   size: nat64;
 };
service : {
  getFileChunk: (text, nat64) -> (opt blob) query;
  getFileInfo: (text) -> (opt FileInfo) query;
  listFiles: () -> (vec text) query;
  uploadFileChunk: (text, text, nat64, nat64, nat64, vec nat8) -> ();
  verifyFileIntegrity: (text) -> (bool) query;
}
